{
  "name": "@xaaxaax/subscription-filter",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xaaxaax/aws-serverless-collecting-logs.git",
    "directory": "packages/subscription-filter"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prebuild": "rm -rf dist build cdk.out",
    "build": "pnpm run build:tsc && pnpm run build:esbuild",
    "build:tsc": "tsc --noemit",
